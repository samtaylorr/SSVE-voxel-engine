; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="SSVE"
config/description="
A Minecraft-style voxel engine prototype built in **Godot 4**, focusing on learning voxel terrain generation, chunking, and engine architecture.	"
run/main_scene="res://scenes/main_menu.tscn"
config/features=PackedStringArray("4.5", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

AudioManager="*res://scripts/audio_manager.gd"
WorldManager="*res://world_manager.gd"
ChunkHelper="*res://scripts/chunk_helper.gd"
SettingsHandler="*res://scripts/settings_handler.gd"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

